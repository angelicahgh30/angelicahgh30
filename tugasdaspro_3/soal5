program Perkalian_Angka;

uses crt;

var
  i, hasil: integer;

begin
  clrscr;
  writeln('Hasil Perkalian Angka dari 1 hingga 100:');
  
  for i := 1 to 100 do
  begin
    hasil := 1 * i; 

    if not ((hasil mod 3 <> 0) and (hasil mod 5 <> 0)) then
    begin
      writeln(hasil); 
    end;
  end;

  readln; 
end.
