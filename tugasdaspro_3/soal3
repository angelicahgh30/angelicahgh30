program Cek_angka_prima;
uses crt;
var
    i, j: integer;
    prima: boolean;

begin
    clrscr;
    writeln('Angka prima dari 1 sampai 50:');
     for i := 2 to 50 do
  begin
    Prima := true; 
    for j := 2 to i div 2 do
    begin
      if (i mod j = 0) then
      begin
        Prima := false; 
        break;
      end;
    end;

    if prima then
      writeln(i);
  end;
  
  readln; 
end.
